            --gradient-start: #6a0dad;
            --gradient-end: #9c27b0;
            
            /* Enhanced vibrant colors */
            --vibrant-purple: #a020f0;
            --vibrant-blue: #1e90ff;
            --vibrant-pink: #ff1493;
            --vibrant-teal: #20b2aa;
            --vibrant-green: #32cd32;
            --vibrant-orange: #ff8c00;
            --vibrant-red: #ff4500;
            --vibrant-yellow: #ffd700;
        }
        
        body {
            background-color: var(--background-dark);
            color: var(--text-color);
            overflow-x: hidden;
            background-image: radial-gradient(circle at 10% 20%, rgba(106, 13, 173, 0.1) 0%, rgba(0, 0, 0, 0) 80%),
                              radial-gradient(circle at 90% 80%, rgba(156, 39, 176, 0.1) 0%, rgba(0, 0, 0, 0) 80%);
        }
        
        .schedule-section h1, 
        .schedule-section h2 {
            color: var(--text-color);
        }
        
        /* Header animations */
        .schedule-section h1 {
            background: linear-gradient(to right, var(--gradient-start), var(--gradient-end));
            -webkit-background-clip: text;
            background-clip: text;
            color: transparent;
            position: relative;
            display: inline-block;
            margin-bottom: 5px;
        }
        
        .schedule-section h1::after {
            content: "";
            position: absolute;
            bottom: -5px;
            left: 0;
            width: 50px;
            height: 3px;
            background: linear-gradient(to right, var(--gradient-start), var(--gradient-end));
            animation: expandWidth 3s ease-in-out infinite;
        }
        
        @keyframes expandWidth {
            0%, 100% { width: 50px; }
            50% { width: 150px; }
        }
        
        /* Promotional Banner */
        .promo-banner {
            background: linear-gradient(135deg, var(--gradient-start), var(--gradient-end));
            padding: 20px;
            border-radius: 10px;
            margin-bottom: 25px;
            position: relative;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.3);
            animation: pulseShadow 4s infinite alternate;
        }
        
        @keyframes pulseShadow {
            from { box-shadow: 0 5px 15px rgba(0, 0, 0, 0.3); }
            to { box-shadow: 0 10px 25px rgba(138, 43, 226, 0.5); }
        }
        
        .promo-banner::before {
            content: "";
            position: absolute;
            top: -50%;
            left: -50%;
            width: 200%;
            height: 200%;
            background: radial-gradient(circle, rgba(255,255,255,0.1) 0%, rgba(255,255,255,0) 70%);
            transform: rotate(30deg);
            animation: shimmer 10s linear infinite;
        }
        
        @keyframes shimmer {
            from { transform: translateX(-50%) rotate(30deg); }
            to { transform: translateX(50%) rotate(30deg); }
        }
        
        .promo-banner h3 {
            margin-top: 0;
            font-size: 1.5rem;
            font-weight: 700;
            margin-bottom: 10px;
