            align-items: center;
            justify-content: center;
            width: 24px;
            height: 24px;
            border-radius: 50%;
            margin-right: 8px;
            font-size: 12px;
            font-weight: 700;
            box-shadow: 0 2px 6px rgba(0,0,0,0.3);
            position: relative;
            overflow: hidden;
        }
        
        .grade-indicator::after {
            content: "";
            position: absolute;
            top: -50%;
            left: -50%;
            width: 200%;
            height: 200%;
            background: rgba(255,255,255,0.15);
            transform: rotate(45deg);
            animation: shimmerGrade 2s infinite linear;
        }
        
        @keyframes shimmerGrade {
            from { transform: rotate(45deg) translateX(-100%); }
            to { transform: rotate(45deg) translateX(100%); }
        }
        
        .grade-a .grade-indicator {
            background: linear-gradient(135deg, #32cd32, #00fa9a);
