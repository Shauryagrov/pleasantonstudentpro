    <script>
        // Fast-XML-Parser equivalent (simplified for this implementation)
        function parseXML(xmlStr) {
            const parser = new DOMParser();
            const xmlDoc = parser.parseFromString(xmlStr, "text/xml");
            return xmlToJson(xmlDoc);
        }

        function xmlToJson(xml) {
            // Create the return object
            var obj = {};

            if (xml.nodeType == 1) {
                // Element
                // Do attributes
                if (xml.attributes.length > 0) {
                    obj["@attributes"] = {};
                    for (var j = 0; j < xml.attributes.length; j++) {
                        var attribute = xml.attributes.item(j);
                        obj["@attributes"][attribute.nodeName] = attribute.nodeValue;
                    }
                }
            } else if (xml.nodeType == 3) {
                // Text
                obj = xml.nodeValue;
            }

            // Do children
            if (xml.hasChildNodes()) {
                for (var i = 0; i < xml.childNodes.length; i++) {
                    var item = xml.childNodes.item(i);
                    var nodeName = item.nodeName;
                    if (typeof(obj[nodeName]) == "undefined") {
                        obj[nodeName] = xmlToJson(item);
                    } else {
                        if (typeof(obj[nodeName].push) == "undefined") {
                            var old = obj[nodeName];
                            obj[nodeName] = [];
                            obj[nodeName].push(old);
                        }
                        obj[nodeName].push(xmlToJson(item));
                    }
                }
            }
            return obj;
        }

        class StudentAccount {
            constructor(domain, userID, password) {
                this.domain = domain;
                this.userID = userID;
                this.password = password;
            }

            async soapRequest(operation, methodName, params = {}) {
                // Create a simplified XML string for parameters
                let paramStr = JSON.stringify(params)
                    .replace(/"/g, '')
                    .replace(/:/g, '=')
                    .replace(/,/g, ' ')
                    .replace(/{/g, '<Params ')
                    .replace(/}/g, ' />')
                    .replace(/</g, '&lt;')
                    .replace(/>/g, '&gt;');

                try {
                    console.log(`Sending ${operation} request to ${this.domain} for method ${methodName}`);
                    const res = await fetch(`https://${this.domain}/Service/PXPCommunication.asmx`, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/soap+xml; charset=utf-8' },
                        body: `<?xml version="1.0" encoding="utf-8"?>
                        <soap12:Envelope xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap12="http://www.w3.org/2003/05/soap-envelope">
                            <soap12:Body>
                                <${operation} xmlns="http://edupoint.com/webservices/">
                                    <userID>${this.userID}</userID>
                                    <password>${this.password}</password>
                                    <skipLoginLog>true</skipLoginLog>
                                    <parent>false</parent>
                                    <webServiceHandleName>PXPWebServices</webServiceHandleName>
                                    <methodName>${methodName}</methodName>
                                    <paramStr>${paramStr}</paramStr>
                                </${operation}>
                            </soap12:Body>
                        </soap12:Envelope>`
                    });

                    const text = await res.text();
                    console.log('Response received from server');
                    
                    // Log a sample of the response for debugging
                    console.log('Response sample:', text.substring(0, 200) + '...');
                    
                    // First, check if there's an error in the SOAP response
                    if (text.includes('RT_ERROR')) {
                        const errorMatch = text.match(/<ERROR_MESSAGE>(.*?)<\/ERROR_MESSAGE>/);
                        if (errorMatch && errorMatch[1]) {
                            throw new Error(errorMatch[1]);
                        } else {
                            throw new Error('Unknown error in the response');
                        }
                    }
                    
                    // Use regex to extract the XML content from the SOAP response
                    // The StudentVUE API returns the XML as CDATA or escaped HTML inside the Result element
                    const xmlPattern = new RegExp(`<${operation}Result>(?:<!\[CDATA\[)?(.*?)(?:\]\]>)?</${operation}Result>`, 's');
                    const match = text.match(xmlPattern);
                    
                    if (!match || !match[1]) {
                        console.error('Could not extract result from SOAP response');
                        console.log('Full response:', text);
                        
                        // Try a more lenient approach
                        const altPattern = new RegExp(`<${operation}Result>(.*)</${operation}Result>`, 's');
                        const altMatch = text.match(altPattern);
                        if (!altMatch || !altMatch[1]) {
                            throw new Error('Invalid response format from server');
                        }
                        
                        // If we got here, we found something with the alternative pattern
                        const resultXML = this.decodeHtmlEntities(altMatch[1]);
                        console.log('Extracted XML result with alternative pattern');
                        
                        // For StudentInfo, we just need to check login success
                        if (methodName === 'StudentInfo') {
                            return { success: true };
                        }
                        
                        // Try to manually extract data for Gradebook
                        if (methodName === 'Gradebook') {
                            return this.parseXMLDirectly(resultXML, methodName);
                        }
                        
                        return { success: true };
                    }
                    
                    const resultXML = this.decodeHtmlEntities(match[1]);
                    console.log('Extracted XML result');
                    
                    // For very specific StudentVUE response types, handle directly
                    if (methodName === 'StudentInfo') {
                        // For StudentInfo, we just need authentication check
                        return { success: true };
                    }
                    
                    if (methodName === 'Gradebook') {
                        // Direct parsing for gradebook data
                        return this.parseXMLDirectly(resultXML, methodName);
                    }
                    
                    // Fall back to returning a success indicator
                    return { success: true };
                    
                } catch (error) {
                    console.error('SOAP Request Error:', error);
                    throw error;
                }
            }
            
            // Decode HTML entities that might be in the XML
            decodeHtmlEntities(input) {
                if (!input || typeof input !== 'string') return '';
                
                // Use browser's built-in HTML decoding
                const doc = new DOMParser().parseFromString(input, 'text/html');
                return doc.documentElement.textContent;
            }
            
            // Direct XML parsing for StudentVue's format
            parseXMLDirectly(xmlString, methodName) {
                console.log('Using direct XML parsing for', methodName);
                
                // Clean the XML string - StudentVUE sometimes has extra characters
                xmlString = xmlString.trim();
                
                // For debugging
                console.log('XML string sample:', xmlString.substring(0, 100) + '...');
                
                if (methodName === 'Gradebook') {
                    try {
                        // First, try using the browser's XML parser
                        const parser = new DOMParser();
                        const xmlDoc = parser.parseFromString(`<root>${xmlString}</root>`, "text/xml");
                        
                        if (!xmlDoc.querySelector('parsererror')) {
                            // If parsing succeeded, process the XML document
                            const courses = [];
                            const courseNodes = xmlDoc.querySelectorAll('Course');
                            
                            courseNodes.forEach(courseNode => {
                                const title = courseNode.getAttribute('Title') || 'Unknown Course';
                                
                                // Find mark node
                                const markNode = courseNode.querySelector('Mark');
                                if (markNode) {
                                    const score = markNode.getAttribute('CalculatedScoreString') || 'N/A';
                                    const scoreRaw = parseFloat(markNode.getAttribute('CalculatedScoreRaw') || '0');
                                    
                                    // Extract assignments
                                    const assignments = [];
                                    const assignmentNodes = markNode.querySelectorAll('Assignment');
                                    assignmentNodes.forEach(assignmentNode => {
                                        assignments.push({
                                            Measure: assignmentNode.getAttribute('Measure') || 'Unnamed Assignment',
                                            Type: assignmentNode.getAttribute('Type') || 'Uncategorized',
                                            Points: assignmentNode.getAttribute('Points') || 'Not Graded',
                                            PointsPossible: assignmentNode.getAttribute('PointsPossible') || '0',
                                            DueDate: assignmentNode.getAttribute('DueDate') || ''
                                        });
                                    });
                                    
                                    courses.push({
                                        Title: title,
                                        Mark: {
                                            CalculatedScoreString: score,
                                            CalculatedScoreRaw: scoreRaw,
                                            Assignments: assignments
                                        }
                                    });
                                }
                            });
                            
                            // Get reporting periods
                            const reportingPeriods = [];
                            const periodNodes = xmlDoc.querySelectorAll('ReportPeriod');
                            periodNodes.forEach(periodNode => {
                                reportingPeriods.push({
                                    GradePeriod: periodNode.getAttribute('GradePeriod') || 'Unknown Period'
                                });
                            });
                            
                            // Get current period
                            const reportingPeriodNode = xmlDoc.querySelector('ReportingPeriod');
                            const currentPeriod = reportingPeriodNode ? 
                                reportingPeriodNode.getAttribute('GradePeriod') || 'Current Period' : 
                                'Current Period';
                            
                            return {
                                Courses: { Course: courses },
                                ReportingPeriods: { ReportPeriod: reportingPeriods.length > 0 ? reportingPeriods : [{ GradePeriod: 'Current Period' }] },
                                ReportingPeriod: { GradePeriod: currentPeriod }
                            };
                        }
                    } catch (error) {
                        console.error('DOM parsing error:', error);
                        // Continue to fallback regex parsing
                    }
                    
                    // Fallback to regex-based parsing
                    try {
                        // Extract courses using regex
                        const courses = [];
                        const courseRegex = /<Course\s+([^>]*)>([\s\S]*?)<\/Course>/g;
                        let courseMatch;
                        
                        while ((courseMatch = courseRegex.exec(xmlString)) !== null) {
                            try {
                                const courseAttrs = courseMatch[1];
                                const courseContent = courseMatch[2];
                                
                                // Extract course title
                                const titleMatch = courseAttrs.match(/Title="([^"]*)"/);
                                const title = titleMatch ? titleMatch[1] : "Unknown Course";
                                
                                // Extract mark data
                                const markRegex = /<Mark\s+([^>]*)>([\s\S]*?)<\/Mark>/;
                                const markMatch = courseContent.match(markRegex);
                                
                                if (markMatch) {
                                    const markAttrs = markMatch[1];
                                    const markContent = markMatch[2];
                                    
                                    // Extract grade
                                    const scoreMatch = markAttrs.match(/CalculatedScoreString="([^"]*)"/);
                                    const scoreRawMatch = markAttrs.match(/CalculatedScoreRaw="([^"]*)"/);
                                    
                                    const score = scoreMatch ? scoreMatch[1] : "N/A";
                                    const scoreRaw = scoreRawMatch ? parseFloat(scoreRawMatch[1]) : 0;
                                    
                                    // Extract assignments
                                    const assignments = [];
                                    const assignmentRegex = /<Assignment\s+([^>]*?)\/?>/g;
                                    let assignmentMatch;
                                    
                                    while ((assignmentMatch = assignmentRegex.exec(markContent)) !== null) {
                                        try {
                                            const assignmentAttrs = assignmentMatch[1];
                                            
                                            const measureMatch = assignmentAttrs.match(/Measure="([^"]*)"/);
                                            const typeMatch = assignmentAttrs.match(/Type="([^"]*)"/);
                                            const pointsMatch = assignmentAttrs.match(/Points="([^"]*)"/);
                                            const pointsPossibleMatch = assignmentAttrs.match(/PointsPossible="([^"]*)"/);
                                            const dateMatch = assignmentAttrs.match(/DueDate="([^"]*)"/);
                                            
                                            assignments.push({
                                                Measure: measureMatch ? measureMatch[1] : "Unnamed Assignment",
                                                Type: typeMatch ? typeMatch[1] : "Uncategorized",
                                                Points: pointsMatch ? pointsMatch[1] : "Not Graded",
                                                PointsPossible: pointsPossibleMatch ? pointsPossibleMatch[1] : "0",
                                                DueDate: dateMatch ? dateMatch[1] : ""
                                            });
                                        } catch (assignmentError) {
                                            console.error('Error processing assignment:', assignmentError);
                                        }
                                    }
                                    
                                    courses.push({
                                        Title: title,
                                        Mark: {
                                            CalculatedScoreString: score,
                                            CalculatedScoreRaw: scoreRaw,
                                            Assignments: assignments
                                        }
                                    });
                                }
                            } catch (courseError) {
                                console.error('Error processing course:', courseError);
                            }
                        }
                        
                        // Extract reporting periods
                        const reportingPeriods = [];
                        const periodRegex = /<ReportPeriod\s+([^>]*?)\/?>/g;
                        let periodMatch;
                        
                        while ((periodMatch = periodRegex.exec(xmlString)) !== null) {
                            try {
                                const periodAttrs = periodMatch[1];
                                const gradePeriodMatch = periodAttrs.match(/GradePeriod="([^"]*)"/);
                                if (gradePeriodMatch) {
                                    reportingPeriods.push({
                                        GradePeriod: gradePeriodMatch[1]
                                    });
                                }
                            } catch (periodError) {
                                console.error('Error processing period:', periodError);
                            }
                        }
                        
                        // Extract current period
                        const currentPeriodMatch = xmlString.match(/<ReportingPeriod\s+([^>]*?)GradePeriod="([^"]*)"/);
                        const currentPeriod = currentPeriodMatch ? currentPeriodMatch[2] : "Current Period";
                        
                        // If no courses were found, but we have valid XML, create an empty gradebook structure
                        return {
                            Courses: { Course: courses.length > 0 ? courses : [] },
                            ReportingPeriods: { ReportPeriod: reportingPeriods.length > 0 ? reportingPeriods : [{ GradePeriod: 'Current Period' }] },
                            ReportingPeriod: { GradePeriod: currentPeriod }
                        };
                    } catch (regexError) {
                        console.error('Regex parsing error:', regexError);
                        
                        // Return an empty gradebook structure as last resort
                        return {
                            Courses: { Course: [] },
                            ReportingPeriods: { ReportPeriod: [{ GradePeriod: 'Current Period' }] },
                            ReportingPeriod: { GradePeriod: 'Current Period' }
                        };
                    }
                }
                
                // For StudentInfo or other methods, return a simple result
                return { success: true };
            }

            async request(methodName, params = {}) {
                return this.soapRequest('ProcessWebServiceRequest', methodName, params);
            }

            async checkLogin() {
                try {
                    await this.request('StudentInfo');
                    return true;
                } catch (error) {
                    return false;
                }
            }

            async gradebook(reportPeriod) {
                if (reportPeriod) {
                    return this.request('Gradebook', { ReportPeriod: reportPeriod });
                }
                return this.request('Gradebook');
            }
        }

