                            // Reset the score display
                            const scoreElement = item.querySelector('.assignment-score');
                            if (scoreElement && original.pointsEarned !== null && original.pointsPossible !== null) {
                                const scorePercent = (original.pointsEarned / original.pointsPossible) * 100;
                                scoreElement.innerHTML = `${original.pointsEarned}/${original.pointsPossible} (${scorePercent.toFixed(1)}%)`;
                            }
                        }
                    });
                }
                
                // Function to update overall grade based on current assignments
                function updateOverallGrade() {
                    if (!hypotheticalMode) return;
                    
                    // Check if the course uses weighted categories
                    const hasWeightedCategories = !!mark.GradeCalculationSummary && 
                                                  !!mark.GradeCalculationSummary.AssignmentGradeCalc;
                    
                    if (hasWeightedCategories) {
                        // For weighted categories, calculate using category weights
                        updateGradeWithWeightedCategories();
                    } else {
                        // For total points calculation, sum all points
                        updateGradeWithTotalPoints();
                    }
                }
                
                function updateGradeWithWeightedCategories() {
                    // Get all assignments grouped by category
                    const categories = new Map();
                    
                    // Get the category weights from the original grade calculation
                    const categoryWeights = new Map();
                    if (mark.GradeCalculationSummary && mark.GradeCalculationSummary.AssignmentGradeCalc) {
                        const gradeCalcs = Array.isArray(mark.GradeCalculationSummary.AssignmentGradeCalc) 
                            ? mark.GradeCalculationSummary.AssignmentGradeCalc 
                            : [mark.GradeCalculationSummary.AssignmentGradeCalc];
                            
                        gradeCalcs.forEach(calc => {
                            if (calc._Type !== 'TOTAL') {
                                categoryWeights.set(calc._Type, parseFloat(calc._Weight));
                            }
                        });
                    }
                    
                    // Group assignments by category and calculate points
                    document.querySelectorAll('.assignment-item').forEach(item => {
                        const categoryEl = item.querySelector('.assignment-category');
                        if (!categoryEl) return;
                        
                        const category = categoryEl.textContent.trim();
                        const scoreElement = item.querySelector('.assignment-score');
                        if (!scoreElement) return;
                        
                        // Get points from inputs if in hypothetical mode
                        const pointsEarnedInput = scoreElement.querySelector('.points-earned');
                        const pointsPossibleInput = scoreElement.querySelector('.points-possible');
                        
                        if (pointsEarnedInput && pointsPossibleInput) {
                            const earned = parseFloat(pointsEarnedInput.value);
                            const possible = parseFloat(pointsPossibleInput.value);
                            
                            if (!isNaN(earned) && !isNaN(possible) && possible > 0) {
                                // Add to category totals
                                if (!categories.has(category)) {
                                    categories.set(category, { earned: 0, possible: 0 });
                                }
                                
                                const categoryData = categories.get(category);
                                categoryData.earned += earned;
                                categoryData.possible += possible;
                            }
                        }
                    });
                    
                    // Calculate weighted grade
                    let totalWeightedPercentage = 0;
                    let totalWeight = 0;
                    
                    categories.forEach((data, category) => {
                        if (categoryWeights.has(category)) {
                            const weight = categoryWeights.get(category);
                            const percentage = data.earned / data.possible;
                            totalWeightedPercentage += percentage * weight;
                            totalWeight += weight;
                        }
                    });
                    
                    // Calculate final grade percentage
                    let newGradePercent = 0;
                    if (totalWeight > 0) {
                        newGradePercent = (totalWeightedPercentage / totalWeight) * 100;
                    }
                    
                    // Update grade display
                    updateGradeDisplay(newGradePercent);
                }
                
                function updateGradeWithTotalPoints() {
                    let totalPointsEarned = 0;
                    let totalPointsPossible = 0;
                    
                    document.querySelectorAll('.assignment-item').forEach(item => {
                        // Skip items without score
                        const scoreElement = item.querySelector('.assignment-score');
                        if (!scoreElement) return;
                        
                        // Get points from inputs if they exist
                        const pointsEarnedInput = scoreElement.querySelector('.points-earned');
                        const pointsPossibleInput = scoreElement.querySelector('.points-possible');
                        
                        if (pointsEarnedInput && pointsPossibleInput) {
                            const earned = parseFloat(pointsEarnedInput.value);
                            const possible = parseFloat(pointsPossibleInput.value);
                            
                            if (!isNaN(earned) && !isNaN(possible) && possible > 0) {
                                totalPointsEarned += earned;
                                totalPointsPossible += possible;
                            }
                        }
                    });
                    
                    // Calculate new grade percentage
                    let newGradePercent = 0;
                    if (totalPointsPossible > 0) {
                        newGradePercent = (totalPointsEarned / totalPointsPossible) * 100;
                    }
                    
                    // Update grade display
                    updateGradeDisplay(newGradePercent);
                }
                
                function updateGradeDisplay(newGradePercent) {
                    const gradeDiff = newGradePercent - gradePercent;
                    const letterGrade = getLetterGrade(newGradePercent);
                    const colorClass = getGradeColorClass(newGradePercent);
                    
                    // Update the detail-grade element with the new percentage
                    document.getElementById('detail-grade').innerHTML = `
                        <span class="grade-letter ${colorClass}">${letterGrade}</span>
                        <span class="grade-percentage">${newGradePercent.toFixed(1)}%</span>
                        <span class="grade-diff" style="margin-left: 8px; font-size: 14px; color: ${gradeDiff >= 0 ? '#4caf50' : '#f44336'};">
                            (${gradeDiff >= 0 ? '+' : ''}${gradeDiff.toFixed(2)}%)
                        </span>
                    `;
                    
                    // Progress bar references removed
                }
                
                // Show course detail, hide grades list
                gradesContainer.style.display = 'none';
                courseDetailContainer.style.display = 'block';
                
                // Scroll to top
                window.scrollTo(0, 0);
            }
            
            // Display assignments for a course
            function displayCourseAssignments(mark, assignmentsContainer, courseGradePercent) {
                try {
                    // Find assignments in different possible structures
                    let assignments = [];
                    
                    if (mark.Assignments && mark.Assignments.Assignment) {
                        assignments = ensureArray(mark.Assignments.Assignment);
                    } else if (mark.Assignments) {
                        assignments = ensureArray(mark.Assignments);
                    } else if (Array.isArray(mark)) {
                        assignments = mark;
                    }
                    
                    console.log('Assignments:', assignments);
                    
                    if (!assignments || assignments.length === 0) {
                        assignmentsContainer.innerHTML = '<div style="padding:10px; text-align:center; color: #aaa;">No assignments found for this course.</div>';
                        return;
                    }
                    
                    // Sort by date (newest first)
                    assignments.sort((a, b) => {
                        const getDate = (item) => {
                            if (!item) return new Date(0);
                            
                            let dateStr = '';
                            if (item.DueDate) {
                                dateStr = item.DueDate;
                            } else if (item['@attributes'] && item['@attributes'].DueDate) {
                                dateStr = item['@attributes'].DueDate;
                            }
                            return new Date(dateStr || '1970-01-01');
                        };
                        
                        const dateA = getDate(a);
                        const dateB = getDate(b);
                        return dateB - dateA;
                    });
                    
                    // Add chart container
                    const chartContainer = document.createElement('div');
                    chartContainer.className = 'assignments-chart-container';
                    chartContainer.innerHTML = `
                        <div class="chart-title">Grade Progression</div>
                        <div class="chart-canvas-container">
                            <canvas id="assignments-chart"></canvas>
                        </div>
                        <div class="chart-legend" id="chart-legend"></div>
                    `;
                    assignmentsContainer.appendChild(chartContainer);
                    
                    // Add assignments heading
                    const assignmentsHeading = document.createElement('h3');
                    assignmentsHeading.style.marginTop = '30px';
                    assignmentsHeading.style.marginBottom = '15px';
                    assignmentsHeading.textContent = 'Assignments';
                    assignmentsContainer.appendChild(assignmentsHeading);
                    
                    // Flag for "new" assignments - assignments from last 7 days
                    const oneWeekAgo = new Date();
                    oneWeekAgo.setDate(oneWeekAgo.getDate() - 7);
                    
                    // Process assignments for display and for chart
                    const chartData = {
                        labels: [],
                        scores: [],
                        colors: [],
                        categories: []
                    };
                    
                    const categoryColors = {};
                    const categoryList = new Set();
                    
                    assignments.forEach((assignment, index) => {
                        if (!assignment) return; // Skip undefined assignments
                        
                        try {
                            // Log the raw assignment data for debugging
                            console.log(`Assignment ${index}:`, assignment);
                            
                            // Get values from either direct properties or attributes
                            const getProperty = (obj, prop) => {
                                if (!obj) return '';
                                if (obj[prop] !== undefined) return obj[prop];
                                if (obj['@attributes'] && obj['@attributes'][prop] !== undefined) {
                                    return obj['@attributes'][prop];
                                }
                                return '';
                            };
                            
                            let points = getProperty(assignment, 'Points');
                            let pointsPossible = getProperty(assignment, 'PointsPossible');
                            const name = getProperty(assignment, 'Measure');
                            const category = getProperty(assignment, 'Type');
                            const dueDateStr = getProperty(assignment, 'DueDate');
                            
                            // Handle points in different formats
                            if (points === 'Not Graded' || points === '') {
                                points = 'Not Graded';
                            } else if (points.includes('/')) {
                                // If points already includes slash format like "15/20"
                                const parts = points.split('/');
                                points = parts[0].trim();
                                pointsPossible = parts[1].trim();
                            }
                            
                            const dueDate = new Date(dueDateStr || '');
                            const formattedDate = isNaN(dueDate.getTime()) ? 'No date' : dueDate.toLocaleDateString();
                            const isGraded = points !== 'Not Graded' && points !== '';
                            const isRecent = !isNaN(dueDate.getTime()) && dueDate > oneWeekAgo;
                            
                            let scoreDisplay = 'Not Graded';
                            let scorePercent = 0;
                            
                            if (isGraded) {
                                const earnedPoints = parseFloat(points);
                                const totalPoints = parseFloat(pointsPossible);
                                
                                if (!isNaN(earnedPoints) && !isNaN(totalPoints) && totalPoints > 0) {
                                    scorePercent = (earnedPoints / totalPoints) * 100;
                                    scoreDisplay = `${earnedPoints}/${totalPoints} (${scorePercent.toFixed(1)}%)`;
                                    
                                    // Add to chart data if it's a valid score
                                    if (index < 15) {  // Only show last 15 assignments in chart
                                        // Generate color for category if not exists
                                        if (!categoryColors[category]) {
                                            const hue = Math.floor(Math.random() * 360);
                                            categoryColors[category] = `hsl(${hue}, 70%, 60%)`;
                                        }
                                        
                                        chartData.labels.push(name.substring(0, 15) + (name.length > 15 ? '...' : ''));
                                        chartData.scores.push(scorePercent);
                                        chartData.colors.push(categoryColors[category]);
                                        chartData.categories.push(category);
                                        categoryList.add(category);
                                    }
                                } else {
                                    scoreDisplay = points;
                                }
                            }
                            
                            const assignmentItem = document.createElement('div');
                            assignmentItem.className = 'assignment-item';
                            // Add a unique id to track for hypothetical mode
                            assignmentItem.setAttribute('data-id', `assignment-${index}-${Date.now()}`);
                            
                            // Determine grade color
                            let scoreColor = '';
                            if (isGraded) {
                                if (scorePercent >= 90) scoreColor = 'color: #4caf50;';
                                else if (scorePercent >= 80) scoreColor = 'color: #8bc34a;';
                                else if (scorePercent >= 70) scoreColor = 'color: #ffc107;';
                                else if (scorePercent >= 60) scoreColor = 'color: #ff9800;';
                                else scoreColor = 'color: #f44336;';
                            }
                            
                            // Format the HTML with better styling
                            let assignmentHTML = `
                                <div class="assignment-header">
                                    <div class="assignment-name">${name}</div>
                                    <div class="assignment-score" style="${scoreColor}">${scoreDisplay}</div>
                                </div>
                                <div class="assignment-details">
                                    <div>
                                        <span class="assignment-category">${category}</span>
                                        <span class="assignment-date">${formattedDate}</span>
                                        ${isRecent ? '<span class="new-badge">New</span>' : ''}
                                    </div>
                                </div>
                            `;
                            
                            assignmentItem.innerHTML = assignmentHTML;
                            
                            // Only show the first 15 assignments by default
                            if (index >= 15) {
                                assignmentItem.style.display = 'none';
                                assignmentItem.classList.add('hidden-assignment');
                            }
                            
                            assignmentsContainer.appendChild(assignmentItem);
                        } catch (assignmentError) {
                            console.error('Error displaying assignment:', assignmentError);
                        }
                    });
                    
                    // Add "Show More" button if there are more than 15 assignments
                    if (assignments.length > 15) {
                        const showMoreButton = document.createElement('button');
                        showMoreButton.className = 'assignments-toggle';
                        showMoreButton.style.width = '100%';
                        showMoreButton.style.marginTop = '10px';
                        showMoreButton.innerHTML = '<i class="fas fa-plus"></i> Show More Assignments';
                        
                        showMoreButton.addEventListener('click', function() {
                            const hiddenAssignments = assignmentsContainer.querySelectorAll('.hidden-assignment');
                            hiddenAssignments.forEach(assignment => {
                                assignment.style.display = 'block';
                                assignment.classList.remove('hidden-assignment');
                            });
                            showMoreButton.style.display = 'none';
                        });
                        
                        assignmentsContainer.appendChild(showMoreButton);
                    }
                    
                    // Create the chart if we have data
                    if (chartData.labels.length > 0) {
                        setTimeout(() => {
                            createAssignmentsChart(chartData, categoryList, categoryColors);
                        }, 100);
                    } else {
                        document.querySelector('.assignments-chart-container').innerHTML = 
                            '<div style="padding:20px; text-align:center; color: #aaa;">No graded assignments available to display in the chart.</div>';
                    }
                    
                } catch (error) {
                    console.error('Error displaying assignments:', error);
                    assignmentsContainer.innerHTML = '<div style="padding:10px; text-align:center; color: #aaa;">Error loading assignments: ' + error.message + '</div>';
                }
            }
            
            // Create chart for assignments
            function createAssignmentsChart(data, categoryList, categoryColors) {
                const ctx = document.getElementById('assignments-chart').getContext('2d');
                
                // Reverse arrays to show oldest to newest (left to right)
                const reversedLabels = [...data.labels].reverse();
                const reversedScores = [...data.scores].reverse();
                const reversedColors = [...data.colors].reverse();
                const reversedCategories = [...data.categories].reverse();
                
                // Store the original assignment data for later use with interactivity
                const assignmentData = [];
                data.labels.forEach((label, index) => {
                    assignmentData.push({
                        name: label,
                        score: data.scores[index],
                        category: data.categories[index],
                        color: data.colors[index]
                    });
                });
                
                // Store the reversed assignment data
                const reversedAssignmentData = [...assignmentData].reverse();
                
                // Create gradient line
                const gradientStroke = ctx.createLinearGradient(0, 0, 600, 0);
                gradientStroke.addColorStop(0, "#6a0dad");
                gradientStroke.addColorStop(1, "#9c27b0");
                
                // Create glow effect
                const glowLine = ctx.createLinearGradient(0, 0, 600, 0);
                glowLine.addColorStop(0, "rgba(106, 13, 173, 0.8)");
                glowLine.addColorStop(1, "rgba(156, 39, 176, 0.8)");

                // Add selected assignment container
                const chartContainer = document.querySelector('.assignments-chart-container');
                
                // Add selected assignment display
                const selectedAssignment = document.createElement('div');
                selectedAssignment.className = 'selected-assignment';
                selectedAssignment.id = 'selected-assignment';
                chartContainer.appendChild(selectedAssignment);
                
                // Add instructions
                const instructions = document.createElement('div');
                instructions.className = 'chart-instructions';
                instructions.textContent = 'Click on any data point to see assignment details';
                chartContainer.appendChild(instructions);
                
                // Create chart
                const assignmentsChart = new Chart(ctx, {
                    type: 'line',
                    data: {
                        labels: reversedLabels,
                        datasets: [
                            {
                                type: 'line',
                                label: 'Grade Trend',
                                data: reversedScores,
                                borderColor: glowLine,
                                backgroundColor: 'transparent',
                                borderWidth: 4,
                                fill: false,
                                pointBackgroundColor: 'white',
                                pointBorderColor: reversedColors,
                                pointBorderWidth: 2,
                                pointRadius: 6,
                                pointHoverRadius: 8,
                                tension: 0.3,
                                pointStyle: 'circle',
                                pointShadowBlur: 10,
                                pointShadowColor: 'rgba(138, 43, 226, 0.5)',
                                /* We'll handle the hover state manually for better visuals */
                                hoverBorderWidth: 3,
                                hoverBackgroundColor: '#fff',
                                hoverBorderColor: '#6a0dad',
                                /* Shadow to create a glow effect */
                                borderCapStyle: 'round',
                                borderJoinStyle: 'round',
                                shadowOffsetX: 0,
                                shadowOffsetY: 0,
                                shadowBlur: 10,
                                shadowColor: 'rgba(138, 43, 226, 0.5)'
                            }
                        ]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            y: {
                                beginAtZero: true,
                                max: 100,
                                grid: {
                                    color: 'rgba(255, 255, 255, 0.05)',
                                    lineWidth: 1
                                },
                                ticks: {
                                    color: '#aaa',
                                    font: {
                                        size: 11
                                    },
                                    callback: function(value) {
                                        return value + '%';
                                    }
                                },
                                border: {
                                    dash: [4, 4]
                                }
                            },
                            x: {
                                grid: {
                                    display: false
                                },
                                ticks: {
                                    color: '#aaa',
                                    maxRotation: 45,
                                    minRotation: 45,
                                    font: {
                                        size: 10
                                    }
                                }
                            }
                        },
                        plugins: {
                            legend: {
                                display: false
                            },
                            tooltip: {
                                enabled: true,
                                backgroundColor: 'rgba(0, 0, 0, 0.8)',
                                titleColor: '#fff',
                                bodyColor: '#fff',
                                callbacks: {
                                    label: function(context) {
                                        return `Score: ${context.raw.toFixed(1)}%`;
                                    },
                                    title: function(tooltipItems) {
                                        const index = tooltipItems[0].dataIndex;
                                        return reversedLabels[index];
                                    }
                                }
                            }
                        },
                        onClick: function(event, elements) {
                            if (elements.length > 0) {
                                const index = elements[0].index;
                                showSelectedAssignment(reversedAssignmentData[index]);
                            } else {
                                // If clicked elsewhere on the chart, hide the selected assignment
                                hideSelectedAssignment();
                            }
                        },
                        animation: {
                            duration: 2000,
                            easing: 'easeOutQuart'
                        }
                    }
                });
                
                // Create custom legend
                const legendContainer = document.getElementById('chart-legend');
                legendContainer.innerHTML = '';
                
                // Add line trend to legend
                const trendLegend = document.createElement('div');
                trendLegend.className = 'legend-item';
                trendLegend.innerHTML = `
                    <div class="legend-color" style="background: linear-gradient(to right, #6a0dad, #9c27b0);"></div>
                    <span>Grade Trend</span>
                `;
                legendContainer.appendChild(trendLegend);
                
                // Add category colors to legend
                categoryList.forEach(category => {
                    const color = categoryColors[category];
                    const legendItem = document.createElement('div');
                    legendItem.className = 'legend-item';
                    legendItem.innerHTML = `
                        <div class="legend-color" style="background-color: ${color};"></div>
                        <span>${category}</span>
                    `;
                    legendContainer.appendChild(legendItem);
                });
                
                // Function to show selected assignment details
                function showSelectedAssignment(assignment) {
                    const selectedAssignmentElement = document.getElementById('selected-assignment');
                    
                    // Format score display
                    let scoreColor = '';
                    if (assignment.score >= 90) scoreColor = 'color: #4caf50;';
                    else if (assignment.score >= 80) scoreColor = 'color: #8bc34a;';
                    else if (assignment.score >= 70) scoreColor = 'color: #ffc107;';
                    else if (assignment.score >= 60) scoreColor = 'color: #ff9800;';
                    else scoreColor = 'color: #f44336;';
                    
                    selectedAssignmentElement.innerHTML = `
                        <div class="selected-assignment-header">
                            <span class="selected-assignment-name">${assignment.name}</span>
                            <span class="selected-assignment-score" style="${scoreColor}">${assignment.score.toFixed(1)}%</span>
                        </div>
                        <div class="selected-assignment-details">
                            <span class="selected-assignment-category">${assignment.category}</span>
                        </div>
                    `;
                    
                    selectedAssignmentElement.style.display = 'block';
                    selectedAssignmentElement.style.borderColor = assignment.color;
                }
                
                // Function to hide selected assignment
                function hideSelectedAssignment() {
                    const selectedAssignmentElement = document.getElementById('selected-assignment');
                    selectedAssignmentElement.style.display = 'none';
                }
            }
            
            // Helper function to ensure we always have an array
            function ensureArray(item) {
                if (!item) return [];
                return Array.isArray(item) ? item : [item];
            }
            
            // Remove class ID from course name
            function removeClassID(title) {
                if (!title) return 'Unknown Course';
                return String(title).replace(/\([^)]*\)$/, '').trim();
            }
            
            // Setup reporting periods dropdown
            function setupReportingPeriods(gradebook, account) {
                try {
                    const periodSelector = document.getElementById('period-selector');
                    const periodDropdown = document.getElementById('period-dropdown');
                    const currentPeriod = document.getElementById('current-period');
                    
                    if (!gradebook.ReportingPeriods || !gradebook.ReportingPeriods.ReportPeriod) {
                        console.log('No reporting periods found');
                        document.getElementById('reporting-period-selector').style.display = 'none';
                        return;
                    }
                    
                    const reportingPeriods = ensureArray(gradebook.ReportingPeriods.ReportPeriod);
                    console.log('Reporting periods:', reportingPeriods);
                    
                    // Get current period from either attribute or property
                    let currentPeriodName = 'Current Period';
                    if (gradebook.ReportingPeriod) {
                        if (gradebook.ReportingPeriod['@attributes'] && gradebook.ReportingPeriod['@attributes'].GradePeriod) {
                            currentPeriodName = gradebook.ReportingPeriod['@attributes'].GradePeriod;
                        } else if (gradebook.ReportingPeriod.GradePeriod) {
                            currentPeriodName = gradebook.ReportingPeriod.GradePeriod;
                        }
                    }
                    
                    currentPeriod.textContent = currentPeriodName;
                    
                    // Create dropdown items
                    periodDropdown.innerHTML = '';
                    reportingPeriods.forEach((period, index) => {
                        // Get period name from either attribute or property
                        let periodName = 'Period ' + (index + 1);
                        if (period['@attributes'] && period['@attributes'].GradePeriod) {
                            periodName = period['@attributes'].GradePeriod;
                        } else if (period.GradePeriod) {
                            periodName = period.GradePeriod;
                        }
                        
                        const item = document.createElement('div');
                        item.className = 'dropdown-item';
                        item.style.padding = '10px';
                        item.style.cursor = 'pointer';
                        item.style.backgroundColor = 'white';
                        item.style.borderBottom = '1px solid #eee';
                        item.textContent = periodName;
                        
                        item.addEventListener('click', async function() {
                            periodDropdown.style.display = 'none';
                            currentPeriod.textContent = 'Loading...';
                            
                            try {
                                const newGradebook = await account.gradebook(index);
                                displayGrades(newGradebook, account);
                                currentPeriod.textContent = periodName;
                            } catch (error) {
                                console.error('Failed to load period:', error);
                                currentPeriod.textContent = currentPeriodName;
                            }
                        });
                        
                        periodDropdown.appendChild(item);
                    });
                    
                    // Toggle dropdown
                    periodSelector.addEventListener('click', function() {
                        periodDropdown.style.display = periodDropdown.style.display === 'none' ? 'block' : 'none';
                    });
                } catch (error) {
                    console.error('Error setting up period selector:', error);
                    document.getElementById('reporting-period-selector').style.display = 'none';
                }
            }
        });

        // Helper functions for hypothetical mode
        function getLetterGrade(percentage) {
            if (percentage >= 97) return 'A+';
            if (percentage >= 93) return 'A';
            if (percentage >= 90) return 'A-';
            if (percentage >= 87) return 'B+';
            if (percentage >= 83) return 'B';
            if (percentage >= 80) return 'B-';
            if (percentage >= 77) return 'C+';
            if (percentage >= 73) return 'C';
            if (percentage >= 70) return 'C-';
            if (percentage >= 67) return 'D+';
            if (percentage >= 63) return 'D';
            if (percentage >= 60) return 'D-';
            return 'F';
        }
        
        function getGradeColorClass(percentage) {
            if (percentage >= 90) return 'grade-a';
            if (percentage >= 80) return 'grade-b';
            if (percentage >= 70) return 'grade-c';
            if (percentage >= 60) return 'grade-d';
            return 'grade-f';
        }
    </script>
